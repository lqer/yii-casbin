{
    "name": "lqer/yii-casbin",
    "description": "lqer/yii-casbin",
    "type": "librar",
    "require": {
        "php": ">=7.4",
        "composer-plugin-api": "^1.1",
        "casbin/casbin": "dev-master",
        "psr/container": " 1.0.0",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/aliases": "^1.0",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/cache-file": "^3.0@dev",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/db-mysql": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/log-target-file": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
        "autoload": {
        "psr-4": {
            "lqer\\Yii\\Casbin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lqer\\Yii\\Casbin\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-output-dir": "runtime/build/config",
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "providers": "config/providers.php"
        },
        "config-plugin-dev": {
            "tests": "config/tests.php"
        }
    },
    "scripts": {
        "test": [
            "phpunit --colors=always"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lqer",
            "email": "huang_zujian@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": {

    }
}
